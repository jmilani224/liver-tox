                    <td headers="hd_h_Acetaminophen.T2_1_1_1_3" rowspan="1" colspan="1">
                      <bold>&#x0003c;40</bold>
                    </td>
                    <td headers="hd_h_Acetaminophen.T2_1_1_1_4" rowspan="1" colspan="1">
                      <bold>0</bold>
                    </td>
