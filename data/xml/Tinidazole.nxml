                  <given-names>S</given-names>
                </name>
                <name name-style="western">
                  <surname>Gupta</surname>
                  <given-names>GK</given-names>
                </name>
                <name name-style="western">
                  <surname>Wanjari</surname>
                  <given-names>SJ</given-names>
                </name>
                <name name-style="western">
                  <surname>Nijhawan</surname>
