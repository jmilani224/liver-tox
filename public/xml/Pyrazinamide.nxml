                    <th id="hd_b_Pyrazinamide.Te_1_1_4_1" valign="top" align="left" scope="row" rowspan="1" colspan="1">Latency:</th>
                    <td headers="hd_b_Pyrazinamide.Te_1_1_4_1" valign="top" align="left" rowspan="1" colspan="1">2 months</td>
                  </tr>
