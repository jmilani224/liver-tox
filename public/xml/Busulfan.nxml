                    <td headers="hd_b_Busulfan.T2_1_1_4_1" rowspan="1" colspan="1">None</td>
                  </tr>
                  <tr>
                    <th headers="hd_h_Busulfan.T2_1_1_1_1" id="hd_b_Busulfan.T2_1_1_5_1" rowspan="1" colspan="1">Other medications:</th>
